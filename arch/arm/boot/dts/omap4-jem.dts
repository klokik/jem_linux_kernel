			lanes = <
				4 5
				0 1
				2 3
				6 7
				8 9
			>;
		};
	};

	lcd0: display {
		compatible = "hydis,nt71391";
		label = "lcd0";

		enable-gpios = <&gpio2 3 GPIO_ACTIVE_HIGH>;
		cabc-gpios = <&gpio2 5 GPIO_ACTIVE_HIGH>;

		backlight = <&lcd0_bl>;

