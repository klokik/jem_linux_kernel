&mmc5 {
	pinctrl-names = "default";
	pinctrl-0 = <
		&brcmfmac_pins
		&brcmfmac_vmmc_pins
	>;
	/*vmmc-supply = <&brcmfmac_vmmc>;*/
	vmmc-supply = <&vaux1>;

	bus-width = <4>;
	non-removable;
	ti,non-removable;
	/*cap-power-off-card;*/
	/*max-frequency = <50000000>;*/

	#address-cells = <1>;
	#size-cells = <0>;
	brcmf: bcrmf@1 {
		reg = <1>;
		compatible = "brcm,bcm4329-fmac";
		interrupt-parent = <&gpio2>;
		interrupts = <12 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "host-wake";
		brcm,drive-strength = <6>;
	};
};

